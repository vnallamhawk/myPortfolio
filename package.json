    "npm": ">= 2.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "postinstall": "npm run build",
    "bundle": "webpack -p --progress",
    "deploy": "webpack -p --progress --config webpack.deploy.js",
    "lint": "eslint .",
    "prepush": "npm test",
    "security-check": "nsp check --output summary",
    "start": "node app.js",
    "test": "npm run lint && npm run security-check",
    "dev": "webpack-dev-server --hot --history-api-fallback --open"
  },
  "dependencies": {
    "express": "^4.15.2",
    "latest-version": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.0.0",
    "eslint-plugin-react": "^6.0.0",
    "husky": "^0.13.0",
    "moment": "^2.17.0",
    "nsp": "^2.0.0",
    "nuka-carousel": "^2.0.0",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-modal": "^1.0.0",
    "react-router": "^3.0.0",
    "superagent": "^3.0.0",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.0.0",
    "webpack-s3-plugin": "^0.9.0"
  },
  "eslintConfig": {
    "extends": "rakesh/frontend"
  }
